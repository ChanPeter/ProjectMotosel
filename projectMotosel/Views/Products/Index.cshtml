@model IEnumerable<MotoselProject.Models.Product>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PKG)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EHC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SYN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Volume)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PKG)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EHC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SYN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Volume)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SKU }) |
            @Html.ActionLink("Details", "Details", new { id=item.SKU }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SKU })
        </td>
    </tr>
}

</table>
